{% extends "vueMere.html.twig" %}

{% block title %}
	{{ parent() }} - Sign up
{% endblock %}

{% block body %}
	<h3>Formulaire de connexion </h3>

	<div class="well">
		{{ form_start(form) }}
		    {{ form_row(form.username) }}
		    {{ form_row(form.password) }}
		    {{ form_row(form.roles) }}
		    <button type="submit">Register!</button>
		{{ form_end(form) }}
	</div>

{% endblock %}

{% block footer %}
	{{ include("@BayardTestPlatform/Default/footer.html.twig") }}
{% endblock %}